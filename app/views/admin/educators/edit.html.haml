.col-xs-12
  %h2
    = Educator.model_name.human(count: 2)

.col-xs-12
  = simple_form_for [:admin, educator] do |f|
    = f.error_notification
    = f.input :name
    = f.input :registration
    = f.simple_fields_for :user do |u|
      = u.input :email
      = u.input :password
      = u.input :password_confirmation
    = f.button :submit
